<%= simple_form_for(@rodo_cte) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :number %>
    <%= f.input :emitted_at %>
    <%= f.input :series %>
    <%= f.input :amount %>
    <%= f.input :weight %>
    <%= f.input :cnpj %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
